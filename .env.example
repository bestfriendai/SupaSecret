# =============================================================================
# Environment Variables Template
# =============================================================================
# Copy this file to .env and replace ALL placeholder values with real keys
# NEVER commit your .env file with real keys to version control
# =============================================================================

# Supabase Configuration (REQUIRED)
EXPO_PUBLIC_PROJECT_ID=your_project_id_here
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# RevenueCat Configuration (REQUIRED for subscriptions)
EXPO_PUBLIC_REVENUECAT_IOS_KEY=appl_YOUR_IOS_KEY_HERE
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=goog_YOUR_ANDROID_KEY_HERE

# AdMob Configuration (REQUIRED for ads - or use test IDs in development)
# Get your IDs from: https://apps.admob.com/
EXPO_PUBLIC_ADMOB_IOS_APP_ID=ca-app-pub-XXXXXXXXXXXXXXXX~XXXXXXXXXX
EXPO_PUBLIC_ADMOB_ANDROID_APP_ID=ca-app-pub-XXXXXXXXXXXXXXXX~XXXXXXXXXX
EXPO_PUBLIC_ADMOB_IOS_BANNER_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
EXPO_PUBLIC_ADMOB_ANDROID_BANNER_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
EXPO_PUBLIC_ADMOB_IOS_INTERSTITIAL_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
EXPO_PUBLIC_ADMOB_ANDROID_INTERSTITIAL_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
EXPO_PUBLIC_ADMOB_IOS_REWARDED_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX
EXPO_PUBLIC_ADMOB_ANDROID_REWARDED_ID=ca-app-pub-XXXXXXXXXXXXXXXX/XXXXXXXXXX

# AI API Keys (OPTIONAL - for AI features)
EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY=sk-proj-YOUR_OPENAI_KEY_HERE
EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY=sk-ant-api03-YOUR_ANTHROPIC_KEY_HERE
EXPO_PUBLIC_VIBECODE_GROK_API_KEY=xai-YOUR_GROK_KEY_HERE
EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY=YOUR_ELEVENLABS_KEY_HERE

# Analytics Configuration (Optional)
EXPO_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Firebase Configuration (Optional - for analytics and crashlytics)
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_firebase_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID_IOS=your_ios_app_id
EXPO_PUBLIC_FIREBASE_APP_ID_ANDROID=your_android_app_id
